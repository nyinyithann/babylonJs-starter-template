{
  "name": "babylonjs-starter-template",
  "version": "1.0.0",
  "description": "A starter template for BabylonJS project with TypeScript, Webpack, SWC, Jest, Eslint, and Prettier.",
  "license": "MIT",
  "keywords": [
    "babylonjs",
    "typescript",
    "swc",
    "WebGL",
    "3D"
  ],
  "author": "nyi nyi thann",
  "devDependencies": {
    "@svgr/webpack": "^6.2.1",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.151",
    "@swc/helpers": "^0.3.6",
    "@swc/jest": "^0.2.20",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.5.3",
    "stylelint-config-standard": "^25.0.0",
    "swc-loader": "^0.1.15",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "babylonjs": "^4.2.1",
    "babylonjs-gui": "^4.2.1",
    "babylonjs-loaders": "^4.2.1",
    "modern-normalize": "^1.1.0"
  },
  "scripts": {
    "dashboard": "webpack-dashboard -- webpack serve --config build/webpack.js --env env=dev addon=dashboard",
    "lint": "npx eslint --config .eslintrc.js 'src/*.ts' 'src/**/*.ts'",
    "lint:fix": "yarn lint -- --fix",
    "prettier": "npx prettier --config .prettierrc 'src/*.ts' 'src/**/*.ts' --check",
    "prettier:fix": "yarn prettier -- --write",
    "format": "yarn prettier:fix && npm run lint:fix",
    "stylelint": "stylelint --config .stylelintrc.json --fix 'style/*.css' 'src/*.css' 'src/**/*.css' ",
    "build:prod": "NODE_ENV=production && webpack --config build/webpack.js --env env=prod",
    "build:dev": "NODE_ENV=development && webpack --config build/webpack.js --env env=dev",
    "start": "NODE_ENV=development webpack serve --config build/webpack.js --env env=dev"
  }
}
